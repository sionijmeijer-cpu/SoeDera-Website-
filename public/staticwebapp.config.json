{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/sodera-logo.png",
      "/sodera-logo.svg",
      "/favicon.ico",
      "/assets/*",
      "/images/*",
      "/*.{png,jpg,jpeg,gif,svg,ico,webp,css,js,txt,xml,json}"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:;",
    "cache-control": "must-revalidate, max-age=3600"
  }
}
