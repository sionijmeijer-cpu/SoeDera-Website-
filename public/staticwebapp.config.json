{
  "routes": [
    {
      "route": "/*",
      "redirect": "https://www.soedera.eu",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/sodera-logo.png",
      "/sodera-logo.svg",
      "/favicon.ico",
      "/favicon.png",
      "/apple-touch-icon.png",
      "/site.webmanifest",
      "/assets/*",
      "/images/*",
      "/*.{png,jpg,jpeg,gif,svg,ico,webp,css,js,txt,xml,json}"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:;",
    "cache-control": "must-revalidate, max-age=3600"
  },
  "mimeTypes": {
    ".ico": "image/x-icon",
    ".png": "image/png",
    ".svg": "image/svg+xml",
    ".webmanifest": "application/manifest+json"
  }
}
